<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN" monitorInterval="120">
	<Appenders>


		<Console name="Simple-Console">
			<PatternLayout>
				<Pattern>%d{MMdd HH:mm:ss.SSS} %-5p [%c{20}[%t] %m%n</Pattern>
			</PatternLayout>
		</Console>

	</Appenders>

	<Loggers>


		<Root level="INFO" includeLocation="true" additivity="false">
			<AppenderRef ref="Simple-Console"/>
		</Root>
	</Loggers>
</Configuration>