<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lesson.distributed.transaction.atomikos.mapper.slave.UserInfoMapper">

    <select id="selectOne" resultType="com.lesson.distributed.transaction.atomikos.entity.UserInfo">
        select * from user_info limit 1;
    </select>

    <insert id="insert" parameterType="com.lesson.distributed.transaction.atomikos.entity.UserInfo"
            useGeneratedKeys="true" keyProperty="id">
        insert into
           user_info(username,password)
        values
           (#{username},#{password})
    </insert>
</mapper>